#!/bin/sh -e

if [ "$#" -ne 3 ]; then
    cat <<-ENDOFMESSAGE
Please specify the base name of source benchmark file, the benchmark file to be compared and the output file of compare result.
Usage: docker run --rm -t -v `pwd`/pybench-results:/root/results ljishen/pybench compare <source benchmark file> <benchmark file to be compared> <output file of result>
ENDOFMESSAGE
    exit
fi

pybench/pybench.py -s results/$1.pybench -c results/$2.pybench | tee results/$3.prof
