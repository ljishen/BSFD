#!/bin/sh -e

if [ "$#" -ne 1 ]; then
    cat <<-ENDOFMESSAGE
Please specify the file base name for benchmark and output.
Usage: docker run --rm -t -v `pwd`/pybench-results:/root/results ljishen/pybench run <base name of output file>
ENDOFMESSAGE
    exit
fi

pybench/pybench.py -f results/$1.pybench | tee results/$1.prof
